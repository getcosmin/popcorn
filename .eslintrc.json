{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": "off",
        "react/jsx-indent": "off", 
        "react/jsx-filename-extension": [0],
        "react/react-in-jsx-scope": 0,
        "react/jsx-uses-react": 0,
        "require-jsdoc": "off"
    }
}

  
